        <div className="w-full h-full">
          <ReactFlowProvider>
            <FlowEditor initialNodes={nodes} initialEdges={edges} onSave={onFlowChange} />
          </ReactFlowProvider>
        </div>
      </div>
      
      <style jsx global>{`
        .react-flow__node {
          background: transparent !important;
